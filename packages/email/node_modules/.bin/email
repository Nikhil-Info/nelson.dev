#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/nandini/Downloads/nelsonlai.dev/node_modules/.pnpm/react-email@4.3.2/node_modules/react-email/dist/node_modules:/Users/nandini/Downloads/nelsonlai.dev/node_modules/.pnpm/react-email@4.3.2/node_modules/react-email/node_modules:/Users/nandini/Downloads/nelsonlai.dev/node_modules/.pnpm/react-email@4.3.2/node_modules:/Users/nandini/Downloads/nelsonlai.dev/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/nandini/Downloads/nelsonlai.dev/node_modules/.pnpm/react-email@4.3.2/node_modules/react-email/dist/node_modules:/Users/nandini/Downloads/nelsonlai.dev/node_modules/.pnpm/react-email@4.3.2/node_modules/react-email/node_modules:/Users/nandini/Downloads/nelsonlai.dev/node_modules/.pnpm/react-email@4.3.2/node_modules:/Users/nandini/Downloads/nelsonlai.dev/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../react-email/dist/index.js" "$@"
else
  exec node  "$basedir/../react-email/dist/index.js" "$@"
fi
